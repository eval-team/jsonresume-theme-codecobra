{{#if resume.skills.length}}
<div class="skills-container">
  {{#each resume.skills}}
  <section class="container">
    {{#if name}}
      {{> title value=name}}
    {{/if}}
    {{#if level}}
      <h4 class="bold">{{level}}</h4>
    {{/if}}
    {{#if keywords.length}}
      <div class="keywords-container">
        {{#each keywords}}
        <span class="keyword">{{.}}</span>
        {{/each}}
      </div>
    {{/if}}
  </section>
  {{/each}}
</div>
{{/if}}
